{
  "name": "ReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate-assets": "node ./generateAssets.js",
    "postinstall": "node ./bin/postInstall",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ad": "cd android && ENVFILE=.env.dev npx react-native run-android",
    "as": "cd android && ENVFILE=.env.st npx react-native run-android",
    "ap": "cd android && ENVFILE=.env.prd npx react-native run-android",
    "id": "ENVFILE=.env.dev npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "is": "ENVFILE=.env.st npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "ip": "ENVFILE=.env.prd npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "abd": "cd android && ENVFILE=.env.dev ./gradlew bundleRelease",
    "abs": "cd android && ENVFILE=.env.st ./gradlew bundleRelease",
    "abp": "cd android && ENVFILE=.env.prd ./gradlew bundleRelease",
    "convert:bundle": "cd android/app/build/outputs/bundle/release && bundletool build-apks --mode universal --bundle ./app-release.aab --output ./app.apks && mv app.apks app.zip && unzip app.zip && echo /android/app/build/outputs/bundle/release/universal.apk"
  },
